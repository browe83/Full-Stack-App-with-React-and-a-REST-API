rom 'react';\nimport { useHistory } from 'react-router-dom';\nimport { Context } from '../Context';\nimport base64 from 'base-64';\nimport ReactMarkdown from 'react-markdown';\n\nfunction CourseDetail (props) {\n  const [course, setCourse] = useState({});\n  const history = useHistory();\n  const context = useContext(Context);\n  const { authUser } = context;\n\n  useEffect(() => {\n      fetch(`http://127.0.0.1:5000/api/courses/${props.match.params.id}`)\n      .then(res =>  res.json())\n      .then(({ course, status }) => {\n        if (status === 500) {\n          history.push('/error');\n        } else if (status === 404) {\n          history.push('/notfound');\n        } else {\n          setCourse(course);\n        }\n      });\n  }, [props.match.params.id, history])\n\n  function handleDelete(e) {\n    // eslint-disable-next-line no-restricted-globals\n    const confirmDelete = confirm('Are you sure you want to delete this course?');\n    if (confirmDelete) {\n      fetch(`http://127.0.0.1:5000/api/courses/${props.match.params.id}`, {\n        method: 'DELETE',\n        headers: new Headers ({\n          \"Authorization\": `Basic ${base64.encode(`${authUser.emailAddress}:${authUser.password}`)}`,\n          \"Content-Type\": \"application/json\",\n        }),\n      })\n      .then(res => {\n        history.push('/');\n      })\n    } \n  }\n\n  return (\n    <div>\n        <div className=\"actions--bar\">\n          <div className=\"bounds\">\n            <div className=\"grid-100\">\n            { (authUser && (course.userId === authUser.id)) &&\n                <span>\n                  <a className=\"button\" href={`/courses/${props.match.params.id}/update`} >Update Course</a>\n                  {/* eslint-disable-next-line jsx-a11y/anchor-is-valid*/}\n                  <a className=\"button\" onClick={handleDelete} >Delete Course</a>\n                </span>\n            }\n              <a className=\"button button-secondary\" href=\"/\">Return to List</a></div>\n          </div>\n        </div>\n        <div className=\"bounds course--detail\">\n          <div className=\"grid-66\">\n            <div className=\"course--header\">\n              <h4 className=\"course--label\">Course</h4>\n              <h3 className=\"course--title\">{course.title}</h3>\n              <p>By {`${course.userFirstName} ${course.userLastName}`}</p>\n            </div>\n            <div className=\"course--description\">\n              <p>{course.description}</p>\n            </div>\n          </div>\n          <div className=\"grid-25 grid-right\">\n            <div className=\"course--stats\">\n              <ul className=\"course--stats--list\">\n                <li className=\"course--stats--list--item\">\n                  <h4>Estimated Time</h4>\n                  <h3>{course.estimatedTime ? <ReactMarkdown source={course.estimatedTime}/> : 'No estimate available'}</h3>\n                </li>\n                <li className=\"course--stats--list--item\">\n                  <h4>Materials Needed</h4>\n                  <ul>\n                    {(course.materialsNeeded) ? <ReactMarkdown source={course.materialsNeeded}/> : <li>No materials required</li>}\n                  </ul>\n                </li>\n              </ul>\n            </div>\n          </div>\n        </div>\n      </div>\n  )\n}\n\nexport default CourseDetail;","/Users/brandonrowe/Desktop/Coding/full_stack_app_with_react_and_a_rest_api/client/src/components/UserSignIn.js",[],"/Users/brandonrowe/Desktop/Coding/full_stack_app_with_react_and_a_rest_api/client/src/components/UserSIgnUp.js",["76"],"/Users/brandonrowe/Desktop/Coding/full_stack_app_with_react_and_a_rest_api/client/src/components/CreateCourse.js",[],"/Users/brandonrowe/Desktop/Coding/full_stack_app_with_react_and_a_rest_api/client/src/components/UpdateCourse.js",[],"/Users/brandonrowe/Desktop/Coding/full_stack_app_with_react_and_a_rest_api/client/src/components/UserSignOut.js",[],"/Users/brandonrowe/Desktop