ttp://127.0.0.1:5000/api/courses')\n      .then(response => response.json())\n      .then(data => {\n        setCourses(data.filteredCoursesInfo);\n        console.log('this ran')\n      });\n  }, []);\n\n  const courseLinks = () => courses.map(({ course }) => {\n      return (\n            <div key={course.id} className=\"grid-33\"><a className=\"course--module course--link\" href={`/courses/${course.id}`}>\n                <h4 className=\"course--label\">Course</h4>\n                <h3 className=\"course--title\">{course.title}</h3>\n            </a></div>\n      );\n  })\n    return (\n        <div className=\"bounds\">\n            {courseLinks()}\n            <div className=\"grid-33\"><a className=\"course--module course--add--module\" href=\"/courses/create\">\n                <h3 className=\"course--add--title\"><svg version=\"1.1\" xmlns=\"http://www.w3.org/2000/svg\" x=\"0px\" y=\"0px\"\n                    viewBox=\"0 0 13 13\" className=\"add\">\n                    <polygon points=\"7,6 7,0 6,0 6,6 0,6 0,7 6,7 6,13 7,13 7,7 13,7 13,6 \"></polygon>\n                </svg>New Course</h3>\n            </a></div>\n        </div>\n    );\n};\n\nexport default Courses;","/Users/brandonrowe/Desktop/Coding/full_stack_app_with_react_and_a_rest_api/client/src/components/CourseDetail.js",["73"],"import React, { useState, useEffect, useContext } from 'react';\nimport { useHistory } from \"react-router-dom\";\nimport { Context } from '../Context';\nimport base64 from 'base-64';\n\nfunction CourseDetail (props) {\n  const [course, setCourse] = useState({});\n  const history = useHistory();\n  const context = useContext(Context);\n  const { authUser } = context;\n\n  useEffect(() => {\n      fetch(`http://127.0.0.1:5000/api/courses/${props.match.params.id}`)\n      .then(response => response.json())\n      .then(({ course }) => {\n        if (course === null || course === undefined) {\n          history.push('/error');\n        } else {\n          setCourse(course);\n        }\n      });\n  }, [props.match.params.id, history])\n\n  function handleDelete(e) {\n    // eslint-disable-next-line no-restricted-globals\n    const confirmDelete = confirm('Are you sure you want to delete this course?');\n    if (confirmDelete) {\n      fetch(`http://127.0.0.1:5000/api/courses/${props.match.params.id}`, {\n        method: 'DELETE',\n        headers: new Headers ({\n          \"Authorization\": `Basic ${base64.encode(`${authUser.emailAddress}:${authUser.password}`)}`,\n          \"Content-Type\": \"application/json\",\n        }),\n      })\n      .then(res => res.text())\n      .then(res => console.log(res));\n      history.push('/');\n    } else{\n      console.log('false')\n    }\n    \n  }\n\n  return (\n    <div>\n        <div className=\"actions--bar\">\n          <div className=\"bounds\">\n            <div className=\"grid-100\">\n            { (authUser && (course.userId === authUser.id)) &&\n                <span>\n                  <a className=\"button\" href={`/courses/${props.match.params.id}/update`} >Update Course</a>\n                  {/* <a className=\"button\" href=\"/courses/delete\">Delete Course</a> */}\n                  <a className=\"button\" onClick={handleDelete}>Delete Course</a>\n                </span>\n            }\n              <a className=\"button button-secondary\" href=\"/\">Return to List</a></div>\n          </div>\n        </div>\n        <div className=\"bounds course--detail\">\n          <div className=\"grid-66\">\n            <div className=\"course--header\">\n              <h4 className=\"course--label\">Course</h4>\n              <h3 className=\"course--title\">{course.title}</h3>\n              <p>By {`${course.userFirstName} ${course.userLastName}`}</p>\n            </div>\n            <div className=\"course--description\">\n              <p>{course.description}</p>\n            </div>\n          </div>\n          <div className=\"grid-25 grid-right\">\n            <div className=\"course--stats\">\n              <ul className=\"course--stats--list\">\n                <li className=\"course--stats--list--item\">\n                  <h4>Estimated Time</h4>\n                  <h3>{course.estimatedTime ? course.estimatedTime : 'No estimate available'}</h3>\n                </li>\n                <li className=\"course--stats--list--item\">\n                  <h4>Materials Needed</h4>\n                  <ul>\n                    {(course.materialsNeeded > 1) ? <li>{course.materialsNeeded}</li>  : <li>No materials required</li>}\n                  </ul>\n                </li>\n              </ul>\n            </div>\n          </div>\n        </div>\n      </div>\n  )\n}\n\nexport default CourseDetail;","/Users/brandonrowe/Desktop/Coding/full_stack_app_with_react_and_a_rest_api/client/src/components/UserSignIn.js",[],"/Users/brandonrowe/Desktop/Coding/full_stack_app_with_react_and_a_rest_api/client/src/components/UserSIgnUp.js",["74"],"/Users/brandonrowe/Desktop/Coding/full_stack_app_with_react_and_a_rest_api/client/src/components/CreateCourse.js",["75"],"/Users/brandonrowe/Desktop/Coding/full_stack_app_with_react_and_a_rest_api/client/src/components/UpdateCourse.js",[],"/Users/brandonrowe/Desktop/Coding/full_stack_app_with_react_and_a_rest_api/client/src/components/UserSignOut.js",[],"/Users/brandonrowe/Desktop/Coding/full_stack_app_with_react_and_a_rest_api/client/src/Context.js",[],"/Users/brandonrowe/Desktop/Coding/full_stack_app_with_react_and_a_rest_api/client/src/components/PrivateRoute.js",["76"],"/Users/brandonrowe/Desktop/Coding/full_stack_app_with_react_and_a_rest_api/client/src/components/NotFound.js",[],"/Users/brandonrowe/Desktop/Coding/full_stack_app_with_react_and_a_rest_api/client/src/components/UserSignUp.js",["77"],"import React, { useContext, useState } from 'react';\nimport { Context }  from '../Context';\nimport { useHistory } from \"react-router-dom\";\n\nfunction UserSignUp (props) {\n \n  const history = useHistory();\n  const [ firstName, setFirstName ] = useState('');\n  const [ lastName, setLastName ] = useState('');\n  const [ emailAddress, setEmail ] = useState('');\n  const [ password, setPassword ] = useState('');\n  const [ confirm, setConfirm ] = useState('');\n\n  const context = useContext(Context);\n\n  function handleSubmit(e) {\n    e.preventDefault();\n    if (password !== confirm) {\n      console.log('Passwords do not match')\n    } else {\n      console.log('maybe sucessful login');\n      const body = {\n        firstName,\n        lastName,\n        emailAddress,\n        password\n      }\n      // console.log(JSON.stringify(body));\n      // fetch(`http://127.0.0.1:5000/api/users/`, {\n      //      method: 'POST',\n      //      body: JSON.stringify(body),\n      //     }\n      //   )\n      \n        fetch('http://127.0.0.1:5000/api/users/', {\n          method: 'POST', // or 'PUT'\n          headers: {\n            \"Content-Type\": \"application/json\",\n            // \"Accept\": \"application/json\",\n          },\n          body: JSON.stringify(body),\n        })\n        .then(res => {\n          console.log(res.status);\n          return res.json();\n        })\n        .then(data => console.log(data))\n      // context.actions.signIn(emailAddress, password);\n    }\n  }\n  return (\n      <div className=\"bounds\">\n        <div className=\"grid-33 centered signin\">\n          <h1>Sign Up</h1>\n          <div>\n            <form onSubmit={handleSubmit}>\n              <div><input onChange={(e) => setFirstName(e.target.value)} id=\"firstName\" name=\"firstName\" type=\"text\" className=\"\" placeholder=\"First Name\" value={firstName}/></div>\n              <div><input onChange={(e) => setLastName(e.target.value)} id=\"lastName\" name=\"lastName\" type=\"text\" className=\"\" placeholder=\"Last Name\" value={lastName}/></div>\n              <div><input onChange={(e) => setEmail(e.target.value)} id=\"emailAddress\" name=\"emailAddress\" type=\"text\" className=\"\" placeholder=\"Email Address\" value={emailAddress}/></div>\n              <div><input onChange={(e) => setPassword(e.target.value)} id=\"password\" name=\"password\" type=\"password\" className=\"\" placeholder=\"Password\" value={password}/></div>\n              <div><input onChange={(e) => setConfirm(e.target.value)} id=\"confirmPassword\" name=\"confirmPassword\" type=\"password\" className=\"\" placeholder=\"Confirm Password\"\n                  value={confirm}/></div>\n              <div className=\"grid-100 pad-bottom\"><button className=\"button\" type=\"submit\">Sign Up</button><button className=\"button button-secondary\" onClick={context.actions.signIn}>Cancel</button></div>\n            </form>\n          </div>\n          <p>&nbsp;</p>\n          <p>Already have a user account? <a href=\"/signin\">Click here</a> to sign in!</p>\n        </div>\n      </div>\n  )\n}\n\nexport default UserSignUp;\n","/Users/brandonrowe/Desktop/Coding/full_stack_app_with_react_and_a_rest_api/client/src/components/Forbidden.js",[],{"ruleId":"78","replacedBy":"79"},{"ruleId":"80","replacedBy":"81"},{"ruleId":"82","severity":1,"message":"83","line":6,"column":9,"nodeType":"84","messageId":"85","endLine":6,"endColumn":16},{"ruleId":"86","severity":1,"message":"87","line":53,"column":19,"nodeType":"88","endLine":53,"endColumn":64},{"ruleId":"82","severity":1,"message":"89","line":7,"column":9,"nodeType":"84","messageId":"85","endLine":7,"endColumn":16},{"ruleId":"82","severity":1,"message":"90","line":1,"column":27,"nodeType":"84","messageId":"85","endLine":1,"endColumn":36},{"ruleId":"91","severity":1,"message":"92","line":27,"column":6,"nodeType":"93","endLine":27,"endColumn":8,"suggestions":"94"},{"ruleId":"82","severity":1,"message":"89","line":7,"column":9,"nodeType":"84","messageId":"85","endLine":7,"endColumn":16},"no-native-reassign",["95"],"no-negated-in-lhs",["96"],"no-unused-vars","'context' is assigned a value but never used.","Identifier","unusedVar","jsx-a1